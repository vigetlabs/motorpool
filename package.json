{
  "name": "motorpool",
  "version": "0.0.0",
  "description": "Motorpool client-side React/Flux application",
  "scripts": {
    "start": "gulp default"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vigetlabs/motorpool"
  },
  "author": "Ryan Stenberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vigetlabs/motorpool/issues"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "harmony": true,
          "extension": [
            "jsx",
            "js"
          ],
          "visitors": [
            "jstransform/visitors/es6-arrow-function-visitors",
            "jstransform/visitors/es6-class-visitors",
            "jstransform/visitors/es6-object-concise-method-visitors"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "browser-sync": "~1.3.5",
    "browserify": "~4.2.3",
    "envify": "~2.0.1",
    "gulp": "^3.8.0",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-util": "^3.0.0",
    "pretty-hrtime": "~0.2.1",
    "react": "~0.11.0",
    "reactify": "~0.14.0",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.2"
  }
}
